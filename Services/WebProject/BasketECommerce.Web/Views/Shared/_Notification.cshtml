@if (TempData["success"] is not null)
{
    <script src="~/js/index.var.js"></script>

    <script>
        const options = {
            position: "bottom-left",
            durations: {
                global: 2000
            },
            labels: {
                info: "Info"
            }
        };
        let notifier = new AWN(options);
        notifier.info('@TempData["success"]');
    </script>
}

@if (TempData["error"] is not null)
{
    <script src="~/js/index.var.js"></script>

    <script>
        const options = {
            position: "bottom-left",
            durations: {
                global: 2000
            },
            labels: {
                info: "Info"
            }
        };
        let notifier = new AWN(options);
        notifier.warning('@TempData["error"]');
    </script>
}
